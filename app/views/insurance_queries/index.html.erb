

<%= form_for @insurance_query, url: {action: "create"}, html: {class: "nifty-form"} do |f| %>
  <%= f.label :total_annual_income, 'Income & Savings First...', class: 'page-header' %><br>
  <%= f.label :total_annual_income, 
  "Total Annual Income <a id='first-menu' href='#first-content-menu' class='form-label sidr-tag'><i class='fi-info small'></i></a>".html_safe,
   class: 'form-label' %><br>
  <%= f.text_field :total_annual_income, class: 'form-field' %><br>
  <%= f.label :savings,
   "Savings <a id='second-menu' href='#second-content-menu' class='form-label sidr-tag'><i class='fi-info small'></i></a>".html_safe,
    :class => 'form-label' %><br>  
  <%= f.text_field :savings, class: 'form-field' %><br>
  <%= f.label :saved_for_retirement,
   "Saved For Retirement <a id='third-menu' href='#third-content-menu' class='form-label sidr-tag'><i class='fi-info small'></i></a>".html_safe, class: 'form-label' %><br>  
  <%= f.text_field :saved_for_retirement, class: 'form-field' %><br>
  <%= f.submit "Submit", class: 'button [radius round]' %>
<% end %>


